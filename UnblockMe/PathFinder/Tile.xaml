<?xml version="1.0" encoding="UTF-8"?>
<ContentView x:Name="Control" xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" x:Class="UnblockMe.PathFinder.Tile">
    <ContentView.Content>
        <Frame x:Name="TileBackground" Padding="0" BackgroundColor="{Binding TileColor, Source={x:Reference Control}}">
            <Button Clicked="Handle_Clicked"></Button>
        </Frame>
    </ContentView.Content>
</ContentView>
